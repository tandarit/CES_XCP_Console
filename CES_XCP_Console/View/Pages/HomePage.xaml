<Page x:Class="CES_XCP_Console.View.Pages.HomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CES_XCP_Console.View.Pages"
      xmlns:viewmodel="clr-namespace:CES_XCP_Console.ViewModel"
      mc:Ignorable="d" 
      d:DesignHeight="700" d:DesignWidth="950">

    <Page.DataContext>
        <viewmodel:MainWindowVM />
    </Page.DataContext>

    <Grid Background="#FFEEEEEE">
        <ScrollViewer x:Name="infoText" Content="{Binding GetVectorCanInfo}" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="680" Width="930" FontFamily="Segoe Script">
           
        </ScrollViewer>

    </Grid>
</Page>
